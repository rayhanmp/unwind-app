{
  "expo": {
    "name": "Unwind",
    "slug": "Unwind",
    "version": "1.0.0",
    "scheme" : "unwind",
    "orientation": "portrait",
    "icon": "./assets/placeholder.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/placeholder.png",
      "resizeMode": "contain",
      "backgroundColor": "#9768CD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rayhanmp.Unwind"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {}
    },
    "plugins": [
      "expo-router"
    ]
  }
}
